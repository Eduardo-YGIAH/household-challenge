{
  "name": "project3",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.13.1",
    "npm": "6.13.7"
  },
  "main": "./bin/www",
  "scripts": {
    "start": "node bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run client-install && npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.6",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mongoose": "^5.9.3",
    "mongoose-autopopulate": "^0.12.0",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.5",
    "nodemailer-mailgun-transport": "^2.0.0",
    "validator": "^13.7.0"
  }
}